{
  "name": "elegant-bro/stringify",
  "license": [
    "MIT"
  ],
  "authors": [
    {
      "name": "Pavel Stepanets",
      "email": "pahhan.ne@gmail.com"
    },
    {
      "name": "Dmitry Anikeev",
      "email": "anikeev.dmitry@outlook.com"
    }
  ],
  "require": {
    "php": "^7.1 || ^8.0",
    "ext-json": "*",
    "elegant-bro/interfaces": "^1.0.0"
  },
  "require-dev": {
    "ext-simplexml": "*",
    "php-coveralls/php-coveralls": "^2.4",
    "phpunit/phpunit": "^9.5",
    "symplify/easy-coding-standard": "^10.0"
  },
  "autoload": {
    "files": ["src/functions.php"],
    "psr-4": {
      "ElegantBro\\Stringify\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ElegantBro\\Stringify\\Tests\\": "tests"
    }
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
